<div class="forum-container">
    <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Zip Code</th>
          </tr>
        </thead>
        <tbody>
            <% subscribers.forEach(s => {%>
          <tr>
            <td><%= s.name %></td>
            <td><%= s.email %></td>
            <td><%= s.zipCode %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
  </div>
